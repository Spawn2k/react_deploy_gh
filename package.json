{
  "name": "portfolio",
  "private": true,
  "version": "0.0.0",
  "homepage": "https://Spawn2k.github.io/react_deploy_gh",
  "type": "module",
  "scripts": {
    "predeploy": "npm run build",
    "deploy": "gh-pages -d dist",
    "start": "vite --port 5000",
    "dev": "vite",
    "sass": "sass src/assets/scss/main.scss:src/assets/css/main.css -w --style=expanded",
    "build": "vite build",
    "lint": "eslint src --ext js,jsx --report-unused-disable-directives --max-warnings 0",
    "preview": "vite preview",
    "server": "json-server --watch src/data/questions.json --port 8000 --delay 500"
  },
  "dependencies": {
    "@reduxjs/toolkit": "^1.9.5",
    "json-server": "^0.17.3",
    "leaflet": "^1.9.4",
    "react": "^18.2.0",
    "react-datepicker": "^4.16.0",
    "react-dom": "^18.2.0",
    "react-icons": "^4.10.1",
    "react-leaflet": "^4.2.1",
    "react-redux": "^8.1.1",
    "react-router-dom": "^6.14.2",
    "react-scroll": "^1.8.9"
  },
  "devDependencies": {
    "@types/react": "^18.0.37",
    "@types/react-dom": "^18.0.11",
    "@vitejs/plugin-react": "^4.0.0",
    "autoprefixer": "^10.4.14",
    "eslint": "^8.38.0",
    "eslint-plugin-react": "^7.32.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.3.4",
    "gh-pages": "^5.0.0",
    "postcss": "^8.4.27",
    "prettier-plugin-tailwindcss": "^0.2.8",
    "sass": "^1.64.1",
    "tailwindcss": "^3.3.3",
    "vite": "^4.3.9"
  }
}
